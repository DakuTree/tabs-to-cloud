{
	"manifest_version" : 2,

	"name" : "Tabs to Cloud",
	"description" : "Saves chrome tabs in a JSON file on your cloud storage.\nFor times where chrome fucks up your session.",

	"version" : "1.0",

	"icons" : {
		"128" : "icon_128.png"
	},

	"permissions" : [
		"identity", //Used for oAuth
		"storage",  //Used for options
		"alarms",   //Used for refreshing on set interval
		"tabs"      //Used for getting tab data
	],

	"browser_action": {},
	"options_ui" : {
		"page" : "options.html",

		"chrome_style" : true
	},


	"oauth2" : {
		"client_id" : "429174875771-u16vg950aqrtostlg88v5nmnodndooju.apps.googleusercontent.com",
		"scopes" : [
			"https://www.googleapis.com/auth/drive"
		]
	},

	"background" : {
		"scripts" : [
			"lib/jquery-3.1.1.min.js",
			"js/cloudServices.js",
			"js/background.js"
		]
	},

	"minimum_chrome_version" : "42"
}
